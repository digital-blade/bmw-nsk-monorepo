{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "start": {
      "dependsOn": [
        "^start"
      ],
      "cache": false
    },
    "start:prod": {
      "outputs": []
    },
    "test:unit": {
      "outputs": []
    },
    "test:coverage": {
      "outputs": []
    },
    "test:e2e": {
      "outputs": []
    },
    "test:lint": {
      "outputs": []
    }
  }
}
