{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "start": {
      "dependsOn": ["^start"],
      "cache": false
    },
    "start:prod": {
      "dependsOn": ["build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "test:unit": {},
    "test:coverage": {},
    "test:e2e": {},
    "test:lint": {},
    "test:lint-report": {}
  }
}
